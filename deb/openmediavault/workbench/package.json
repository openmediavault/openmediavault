{
  "name": "openmediavault-workbench",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "upgrade": "ncu -u --target minor",
    "start": "ng serve --port 4201",
    "build": "ng build",
    "build:prod": "ng build --configuration production --verbose",
    "test": "jest --watch --no-cache",
    "test:ci": "jest --no-cache --maxWorkers=4",
    "lint:html": "htmllint src/app/**/*.html && html-linter --config html-linter.config.json",
    "lint:tsc": "tsc -p tsconfig.app.json --noEmit && tsc -p tsconfig.spec.json --noEmit",
    "lint:eslint": "ng lint",
    "lint:scss": "stylelint \"**/*.scss\"",
    "lint": "run-p -cl --aggregate-output lint:*",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:scss": "stylelint \"**/*.scss\" --fix",
    "fix": "run-s -cl fix:*",
    "e2e": "ng e2e",
    "i18n": "npm run i18n:extract",
    "i18n:extract": "transloco-keys-manager extract --sort --replace"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.4.0",
    "@angular/cdk": "^13.3.9",
    "@angular/common": "^13.4.0",
    "@angular/compiler": "^13.4.0",
    "@angular/core": "^13.4.0",
    "@angular/flex-layout": "^13.0.0-beta.38",
    "@angular/forms": "^13.4.0",
    "@angular/material": "^13.3.9",
    "@angular/platform-browser": "^13.4.0",
    "@angular/platform-browser-dynamic": "^13.4.0",
    "@angular/router": "^13.4.0",
    "@codemirror/autocomplete": "^6.7.0",
    "@codemirror/commands": "^6.2.4",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/lang-xml": "^6.0.2",
    "@codemirror/language": "^6.6.0",
    "@codemirror/legacy-modes": "^6.3.2",
    "@codemirror/lint": "^6.2.1",
    "@codemirror/search": "^6.4.0",
    "@codemirror/state": "^6.2.0",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@codemirror/view": "^6.11.1",
    "@fontsource/inter": "^4.5.15",
    "@fontsource/roboto-mono": "^4.5.10",
    "@mdi/angular-material": "^7.2.96",
    "@ngneat/transloco": "^3.2.0",
    "@swimlane/ngx-datatable": "20.1.0",
    "@types/chart.js": "^2.9.37",
    "chart.js": "^3.9.1",
    "chartjs-plugin-datalabels": "^2.2.0",
    "core-js": "3.30.2",
    "cronstrue": "^2.27.0",
    "dayjs": "^1.11.7",
    "lodash": "^4.17.21",
    "ngx-scrollbar": "~7.6.1",
    "ngx-toastr": "14.3.0",
    "nunjucks": "^3.2.4",
    "rxjs": "~7.4.0",
    "strip-ansi": "7.0.1",
    "tslib": "^2.5.0",
    "validator": "^13.9.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-builders/jest": "^13.0.4",
    "@angular-devkit/build-angular": "^13.3.11",
    "@angular-eslint/builder": "^13.5.0",
    "@angular-eslint/eslint-plugin": "^13.5.0",
    "@angular-eslint/eslint-plugin-template": "^13.5.0",
    "@angular-eslint/schematics": "^13.5.0",
    "@angular-eslint/template-parser": "^13.5.0",
    "@angular/cli": "^13.3.11",
    "@angular/compiler-cli": "^13.4.0",
    "@angular/language-service": "^13.4.0",
    "@ngneat/transloco-keys-manager": "^3.7.0",
    "@types/jasmine": "^3.10.7",
    "@types/jasminewd2": "^2.0.10",
    "@types/jest": "^27.5.2",
    "@types/lodash": "^4.14.194",
    "@types/node": "^16.18.29",
    "@types/nunjucks": "^3.2.2",
    "@types/validator": "^13.7.17",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "codelyzer": "^6.0.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^36.1.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^1.1.5",
    "html-linter": "^1.1.1",
    "htmllint-cli": "^0.0.7",
    "jasmine-core": "^3.99.1",
    "jasmine-spec-reporter": "^7.0.0",
    "jest": "^27.5.1",
    "jest-canvas-mock": "^2.5.0",
    "jest-preset-angular": "^11.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "protractor": "^7.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.21.0",
    "ts-node": "^10.9.1",
    "typescript": "~4.6.4"
  }
}
