{
  "name": "openmediavault-workbench",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "upgrade": "ncu -u --target minor",
    "start": "ng serve --port 4201",
    "build": "ng build",
    "build:prod": "ng build --configuration production --verbose",
    "test": "jest --watch --no-cache",
    "test:ci": "jest --no-cache",
    "lint:html": "htmllint src/app/**/*.html && html-linter --config html-linter.config.json",
    "lint:tsc": "tsc -p tsconfig.app.json --noEmit && tsc -p tsconfig.spec.json --noEmit",
    "lint:eslint": "ng lint",
    "lint:scss": "stylelint \"**/*.scss\"",
    "lint": "run-p -cl --aggregate-output lint:*",
    "fix:prettier": "prettier --write \"{src,e2e}/**/*.{ts,scss}\"",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:scss": "stylelint \"**/*.scss\" --fix",
    "fix": "run-s -cl fix:*",
    "e2e": "ng e2e",
    "i18n": "npm run i18n:extract",
    "i18n:extract": "ngx-translate-extract --input src --output src/assets/i18n/openmediavault-workbench.pot --clean --sort --format pot"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.16",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "^12.2.16",
    "@angular/compiler": "^12.2.16",
    "@angular/core": "^12.2.16",
    "@angular/flex-layout": "^12.0.0-beta.35",
    "@angular/forms": "^12.2.16",
    "@angular/material": "^12.2.13",
    "@angular/platform-browser": "^12.2.16",
    "@angular/platform-browser-dynamic": "^12.2.16",
    "@angular/router": "^12.2.16",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@codemirror/language": "^6.2.1",
    "@codemirror/lang-json": "^6.0.0",
    "@codemirror/lang-xml": "^6.0.0",
    "@codemirror/legacy-modes": "^6.1.0",
    "@codemirror/state": "^6.1.2",
    "@codemirror/search": "^6.2.1",
    "@codemirror/view": "^6.3.1",
    "@codemirror/autocomplete": "^6.3.0",
    "@codemirror/lint": "^6.0.0",
    "@codemirror/commands": "^6.1.2",
    "@codemirror/theme-one-dark": "^6.1.0",
    "@fontsource/inter": "^4.5.13",
    "@fontsource/roboto-mono": "^4.5.8",
    "@mdi/angular-material": "^6.9.96",
    "@ngx-translate/core": "13.0.0",
    "@swimlane/ngx-datatable": "^20.0.0",
    "@types/chart.js": "^2.9.37",
    "chart.js": "^3.9.1",
    "chartjs-plugin-datalabels": "^2.1.0",
    "core-js": "3.25.5",
    "dayjs": "^1.11.5",
    "lodash": "^4.17.21",
    "ng-block-ui": "^3.0.2",
    "ngx-scrollbar": "~7.6.1",
    "ngx-toastr": "14.1.4",
    "nunjucks": "^3.2.3",
    "rxjs": "~6.6.7",
    "strip-ansi": "7.0.1",
    "tslib": "^2.4.0",
    "validator": "^13.7.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-builders/jest": "^12.1.2",
    "@angular-devkit/build-angular": "^12.2.18",
    "@angular-eslint/builder": "^12.7.0",
    "@angular-eslint/eslint-plugin": "^12.7.0",
    "@angular-eslint/eslint-plugin-template": "^12.7.0",
    "@angular-eslint/schematics": "^12.7.0",
    "@angular-eslint/template-parser": "^12.7.0",
    "@angular/cli": "^12.2.18",
    "@angular/compiler-cli": "^12.2.16",
    "@angular/language-service": "^12.2.16",
    "@biesbjerg/ngx-translate-extract": "7.0.4",
    "@types/jasmine": "^3.10.6",
    "@types/jasminewd2": "^2.0.10",
    "@types/jest": "^27.5.2",
    "@types/lodash": "^4.14.186",
    "@types/node": "^16.11.65",
    "@types/nunjucks": "^3.2.1",
    "@types/validator": "^13.7.7",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "codelyzer": "^6.0.2",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^36.1.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "html-linter": "^1.1.1",
    "htmllint-cli": "^0.0.7",
    "jasmine-core": "^3.99.1",
    "jasmine-spec-reporter": "^7.0.0",
    "jest": "^27.5.1",
    "jest-canvas-mock": "^2.4.0",
    "jest-preset-angular": "^9.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "protractor": "^7.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.21.0",
    "ts-node": "^10.9.1",
    "typescript": "~4.3.5"
  }
}
