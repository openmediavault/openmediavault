			<extraoptions></extraoptions>
			<passwordauthentication>1</passwordauthentication>
			<permitrootlogin>1</permitrootlogin>
			<tcpforwarding>0</tcpforwarding>
			<compression>0</compression>
			<dnssd>
				<enable>1</enable>
				<name>%h - SSH</name>
			</dnssd>
		</ssh>
		<nfs>
			<enable>0</enable>
			<numproc>8</numproc>
			<shares>
