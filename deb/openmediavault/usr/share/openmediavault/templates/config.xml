		<enablessl>0</enablessl>
		<sslport>443</sslport>
		<forcesslonly>0</forcesslonly>
		<sslcertificateref></sslcertificateref>
		<timeout>5</timeout>
		<dnssd>
			<enable>1</enable>
			<name>%h - Web administration</name>
		</dnssd>
	</webadmin>
