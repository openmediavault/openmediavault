msgid "Active"
msgstr "Aktivní"

msgid "Available"
msgstr "Dostupné"

msgid "Capacity"
msgstr "Kapacita"

msgid "Create"
msgstr "Vytvořit"

msgid "Created logical volume '{{ name }}'."
msgstr "Vytvořen logický svazek „{{ name }}“."

msgid "Created physical volume {{ devicefile }}."
msgstr "Vytvořen fyzický svazek „{{ devicefile }}“."

msgid "Created snapshot of logical volume '{{ devicefile }}'."
msgstr "Vytvořen snímek logického svazku „{{ devicefile }}“."

msgid "Created volume group '{{ name }}'."
msgstr "Vytvořena skupina svazků „{{ name }}“."

msgid "Device"
msgstr "Zařízení"

msgid "Devices"
msgstr "Zařízení"

msgid "Do you really want to create a physical volume on '{{ devicefile }}'?"
msgstr "Opravdu chcete vytvořit fyzický svazek na „{{ devicefile }}“?"

msgid "Do you really want to resize the selected physical volume?"
msgstr "Opravdu chcete změnit velikost označeného fyzického svazku?"

msgid "Edit"
msgstr "Upravit"

msgid "Extend"
msgstr "Zvětšit"

msgid "Extended logical volume '{{ devicefile }}'."
msgstr "Zvětšen logický svazek „{{ devicefile }}“."

msgid "Extended volume group '{{ devicefile }}'."
msgstr "Zvětšena skupina svazků „{{ devicefile }}“."

msgid "Free"
msgstr "Volné"

msgid "Invalid name"
msgstr "Neplatný název"

msgid "LVM"
msgstr "SLS"

msgid "Logical Volume Management"
msgstr "Správa logických svazků"

msgid "Logical Volumes"
msgstr "Logické svazky"

msgid "Name"
msgstr "Název"

msgid "Physical Volumes"
msgstr "Fyzické svazky"

msgid "Please wait, a snapshot will be created ..."
msgstr "Počkejte prosím, snímek bude vytvořen…"

msgid "Reduce"
msgstr "Zmenšit"

msgid "Reduced volume group '{{ devicefile }}'."
msgstr "Zmenšena skupina svazků „{{ devicefile }}“."

msgid "Resize"
msgstr "Změnit velikost"

msgid "Select a device ..."
msgstr "Vyberte zařízení…"

msgid "Select a volume group ..."
msgstr "Vyberte skupinu svazků"

msgid "Select devices ..."
msgstr "Vyberte zařízení"

msgid "Select the devices used to extend the volume group."
msgstr "Vyberte zařízení, která přidat do skupiny svazků."

msgid "Select the devices used to reduce from the volume group."
msgstr "Vyberte zařízení, která chcete odebrat ze skupiny svazků."

msgid "Size"
msgstr "Velikost"

msgid "Snapshot"
msgstr "Snímek"

msgid "The name of the logical volume."
msgstr "Název logického svazku."

msgid "The name of the volume group."
msgstr "Název skupiny svazku."

msgid ""
"The percentage of the total space in the volume group to allocate for the "
"new logical volume."
msgstr "Procento z celkového prostoru ve skupině svazků, které přiřadit novému logickému svazku."

msgid ""
"The percentage of the total space in the volume group to use by the logical "
"volume."
msgstr "Procento z celkového prostoru ve skupině svazků, které využít logickým svazkem."

msgid "Updated logical volume '{{ devicefile }}'."
msgstr "Aktualizován logický svazek „{{ devicefile }}“."

msgid "Updated volume group '{{ devicefile }}'."
msgstr "Aktualizována skupina svazků „{{ devicefile }}“."

msgid "Used"
msgstr "Použito"

msgid "Volume Group"
msgstr "Skupina svazků"

msgid "Volume Groups"
msgstr "Skupiny svazků"

msgid "Volume group"
msgstr "Skupina svazků"
