msgid "Active"
msgstr "Aktiv"

msgid "Available"
msgstr "Tillgänglig"

msgid "Capacity"
msgstr "Kapacitet"

msgid "Create"
msgstr "Skapa"

msgid "Created logical volume '{{ name }}'."
msgstr "Skapade den logiska volymen '{{ name }}'."

msgid "Created physical volume {{ devicefile }}."
msgstr "Skapade den fysiska volymen {{ devicefile }}."

msgid "Created snapshot of logical volume '{{ devicefile }}'."
msgstr "Skapade ögonblicksbild av den logiska volymen '{{ devicefile }}'."

msgid "Created volume group '{{ name }}'."
msgstr "Skapade volymgruppen '{{ name }}'."

msgid "Device"
msgstr "Enhet"

msgid "Devices"
msgstr "Enheter"

msgid "Do you really want to create a physical volume on '{{ devicefile }}'?"
msgstr "Vill du verkligen skapa en fysisk volym på '{{ devicefile }}'?"

msgid "Do you really want to resize the selected physical volume?"
msgstr "Vill du verkligen ändra storlek på den valda fysiska volymen?"

msgid "Edit"
msgstr "Redigera"

msgid "Extend"
msgstr "Utöka"

msgid "Extended logical volume '{{ devicefile }}'."
msgstr "Utökade den logiska volymen '{{ devicefile }}'."

msgid "Extended volume group '{{ devicefile }}'."
msgstr "Utökade volymgruppen '{{ devicefile }}'."

msgid "Free"
msgstr "Ledigt"

msgid "Invalid name"
msgstr "Ogiltigt namn"

msgid "LVM"
msgstr "LVM"

msgid "Logical Volume Management"
msgstr "Logisk volymhantering"

msgid "Logical Volumes"
msgstr "Logiska volymer"

msgid "Name"
msgstr "Namn"

msgid "Physical Volumes"
msgstr "Fysiska volymer"

msgid "Please wait, a snapshot will be created ..."
msgstr "Vänta, en ögonblicksbild kommer att skapas ..."

msgid "Reduce"
msgstr "Minska"

msgid "Reduced volume group '{{ devicefile }}'."
msgstr "Minskade volymgruppen '{{ devicefile }}'."

msgid "Resize"
msgstr "Ändra storlek"

msgid "Select a device ..."
msgstr "Välj en enhet ..."

msgid "Select a volume group ..."
msgstr "Välj en volymgrupp ..."

msgid "Select devices ..."
msgstr "Välj enheter ..."

msgid "Select the devices used to extend the volume group."
msgstr "Välj de enheter som används för att utöka volymgruppen."

msgid "Select the devices used to reduce from the volume group."
msgstr "Välj de enheter som används för att minska från volymgruppen."

msgid "Size"
msgstr "Storlek"

msgid "Snapshot"
msgstr "Ögonblicksbild"

msgid "The name of the logical volume."
msgstr "Namnet på den logiska volymen."

msgid "The name of the volume group."
msgstr "Namnet på volymgruppen."

msgid ""
"The percentage of the total space in the volume group to allocate for the "
"new logical volume."
msgstr "Procentandelen av det totala utrymmet i volymgruppen som ska fördelas för den nya logiska volymen."

msgid ""
"The percentage of the total space in the volume group to use by the logical "
"volume."
msgstr "Procentandelen av det totala utrymmet i den volymgrupp som ska användas av den logiska volymen."

msgid "Updated logical volume '{{ devicefile }}'."
msgstr "Uppdaterade den logiska volymen '{{ devicefile }}'."

msgid "Updated volume group '{{ devicefile }}'."
msgstr "Uppdaterade volymgruppen '{{ devicefile }}'."

