msgid "Active"
msgstr "Активний"

msgid "Available"
msgstr "Доступний"

msgid "Capacity"
msgstr "Ємкість"

msgid "Create"
msgstr "Створити"

msgid "Created logical volume '{{ name }}'."
msgstr "Створено логічний том '{{ name }}'."

msgid "Created physical volume {{ devicefile }}."
msgstr "Створено фізичний том {{ devicefile }}."

msgid "Created snapshot of logical volume '{{ devicefile }}'."
msgstr "Створено знімок логічного тому \"{{ devicefile }}\"."

msgid "Created volume group '{{ name }}'."
msgstr "Створено групу томів \"{{ name }}\"."

msgid "Device"
msgstr "Пристрій"

msgid "Devices"
msgstr "Пристрої"

msgid "Do you really want to create a physical volume on '{{ devicefile }}'?"
msgstr "Бажаєте створити фізичний том на '{{ devicefile }}'?"

msgid "Do you really want to resize the selected physical volume?"
msgstr "Бажаєте змінити розмір даного фізичного розділу?"

msgid "Edit"
msgstr "Редагувати"

msgid "Extend"
msgstr "Збільшити"

msgid "Extended logical volume '{{ devicefile }}'."
msgstr "Розширений логічний том '{{ devicefile }}'."

msgid "Extended volume group '{{ devicefile }}'."
msgstr "Розширена група томів \"{{ devicefile }}\"."

msgid "Free"
msgstr "Вільно"

msgid "Invalid name"
msgstr "Неправильна назва"

msgid "LVM"
msgstr "LVM"

msgid "Logical Volume Management"
msgstr "Керування логічними розділами"

msgid "Logical Volumes"
msgstr "Логічні томи"

msgid "Name"
msgstr "Ім'я"

msgid "Physical Volumes"
msgstr "Фізичні томи"

msgid "Please wait, a snapshot will be created ..."
msgstr "Зачекайте, створюється знімок..."

msgid "Reduce"
msgstr "Зменшити"

msgid "Reduced volume group '{{ devicefile }}'."
msgstr "Зменшена група томів \"{{ devicefile }}\"."

msgid "Resize"
msgstr "Змінити розмір"

msgid "Select a device ..."
msgstr "Оберіть пристрій ..."

msgid "Select a volume group ..."
msgstr "Оберіть групу розділів ..."

msgid "Select devices ..."
msgstr "Оберіть пристрої ..."

msgid "Select the devices used to extend the volume group."
msgstr "Виберіть пристрої, які використовуються для розширення групи томів."

msgid "Select the devices used to reduce from the volume group."
msgstr "Виберіть пристрої, які використовуються для зменшеної групи томів."

msgid "Size"
msgstr "Розмір"

msgid "Snapshot"
msgstr "Знімок"

msgid "The name of the logical volume."
msgstr "Ім'я логічного тому."

msgid "The name of the volume group."
msgstr "Ім'я групи томів."

msgid ""
"The percentage of the total space in the volume group to allocate for the "
"new logical volume."
msgstr "Відсоток загального простору в групі томів, який потрібно виділити для нового логічного тому."

msgid ""
"The percentage of the total space in the volume group to use by the logical "
"volume."
msgstr "Відсоток загального простору в групі томів для використання логічним томом."

msgid "Updated logical volume '{{ devicefile }}'."
msgstr "Оновлено логічний том \"{{ devicefile }}\"."

msgid "Updated volume group '{{ devicefile }}'."
msgstr "Оновлена група томів \"{{ devicefile }}\"."

msgid "Used"
msgstr "Використано"

msgid "Volume Group"
msgstr "Група Розділів"

msgid "Volume Groups"
msgstr "Групи Розділів"

msgid "Volume group"
msgstr "Група розділів"
