{
	"type": "config",
	"id": "conf.service.ftp.modtls",
	"title": "FTP modtls settings",
	"queryinfo": {
		"xpath": "//services/ftp/modules/mod_tls",
		"iterable": false
	},
	"properties": {
		"enable": {
			"type": "boolean",
			"default": false
		},
		"required": {
			"type": "boolean",
			"default": false
		},
		"sslcertificateref": {
			"type": "string",
			"oneOf": [{
				"type": "string",
				"format": "uuidv4"
			},{
				"type": "string"
			}],
			"default": ""
		},
		"nosessionreuserequired": {
			"type": "boolean",
			"default": false
		},
		"useimplicitssl": {
			"type": "boolean",
			"default": false
		},
		"extraoptions": {
			"type": "string",
			"default": ""
		}
	}
}
