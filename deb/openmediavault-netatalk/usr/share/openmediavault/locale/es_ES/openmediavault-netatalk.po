msgid "Add share"
msgstr "Añadir compartido"

msgid "Advertise this service via mDNS/DNS-SD."
msgstr ""

msgid "Allow guest logins"
msgstr "Permitir acceso de invitado"

msgid "Apple Filing"
msgstr "Apple Filing"

msgid "Authentication"
msgstr "Autenticación"

msgid "Case folding"
msgstr "Conversión a mayúsculas"

msgid "Client sees lowercase, server sees uppercase"
msgstr "Los clientes ven minúsculas, el servidor mayúsculas"

msgid "Client sees uppercase, server sees lowercase"
msgstr "Los clientes ven mayúsculas, el servidor minúsculas"

msgid "Comment"
msgstr "Comentario"

msgid "DNS Service Discovery"
msgstr ""

