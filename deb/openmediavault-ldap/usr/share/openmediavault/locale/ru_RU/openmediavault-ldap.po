msgid "Base DN"
msgstr "Основное уникальное имя"

msgid "Directory Service"
msgstr "Служба каталогов"

msgid "Enable"
msgstr "Включить"

msgid "Extra options"
msgstr "Дополнительные опции"

msgid "General settings"
msgstr "Общие параметры"

msgid "Groups suffix"
msgstr "Суффикс групп"

msgid "Host"
msgstr "Хост"

